{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "front": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached  --content-base --port=3000",
    "start": "npm run front",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.8",
    "systemjs": "0.19.22",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "bootstrap": "^3.3.6",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "lite-server": "^2.1.0",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.5.0",
    "tslint-loader": "^2.1.3",
    "typescript": "^1.8.2",
    "typings": "^0.6.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
